{
  "expo": {
    "name": "Family Humanitarian Experience",
    "slug": "Family Humanitarian Experience",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "Family Humanitarian Experience",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "splash": {
      "image": "./assets/images/android-chrome-192x192.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B3F73"
    },

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-chrome-192x192.png",
        "backgroundColor": "#0B3F73"
      },
      "edgeToEdgeEnabled": true
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/android-chrome-192x192.png",
          "resizeMode": "contain",
          "backgroundColor": "#0B3F73"
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    }
  }
}
